{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
  "include": ["env.d.ts", "src/**/*", "src/**/*.vue"],
  "exclude": ["src/**/__tests__/*"],
  "compilerOptions": {
    "module": "esnext",
    "moduleResolution": "node",
    "target": "esnext",
    "lib": ["esnext"],
    "strict": true,
    "sourceMap": true,
    "noEmitOnError": true,
    "baseUrl": ".",
    "resolveJsonModule": true,
    "outDir": "out"
}
}
